{
    "extends": [
        "xo",
        "xo-react"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": [2, { "argsIgnorePattern": "^_" }],
        "indent": [2, 4, {"SwitchCase": 1}],
        "no-undef": 0,
        "no-unused-vars": 0,
        "react/jsx-indent": [2, 4, { "checkAttributes": true }],
        "react/jsx-fragments": [2, "element"]
    }
}
